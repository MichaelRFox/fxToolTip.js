{
  "name": "fx.tooltip.js",
  "version": "2.0.0",
  "description": "A tooltip library for javascript",
  "keywords": [
    "tooltip",
    "d3.js"
  ],
  "homepage": "https://github.com/MichaelRFox/fxToolTip.js/blob/master/README.md",
  "bugs": {
    "url": "https://github.com/MichaelRFox/fxToolTip.js/issues",
    "email": "fox.michael.r@gmail.com"
  },
  "license": "MIT",
