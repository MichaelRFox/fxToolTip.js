    "name": "Michael R. Fox, Ph.D.",
    "email": "fox.michael.r@gmail.com"
  },
  "main": "dist/index.js",
  "module": "src/index.js",
  "directories": {
    "test": "test",
    "dist": "dist",
    "src": "src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MichaelRFox/fxToolTip.js.git"
  },
  "scripts": {
    "build": "npm run rollupBuild && npm run terser",
