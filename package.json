    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.5",
    "browserslist": "^4.17.1",
    "core-js": "^3.18.0",
    "rollup": "^2.57.0",
    "terser": "^5.9.0"
  },
  "dependencies": {},
  "engines": {
    "node": ">=14 <15",
    "npm": ">=6"
  }
