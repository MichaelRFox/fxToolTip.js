{
  "name": "fx.tooltip.js",
  "version": "1.1.0",
  "description": "A tooltip library for javascript",
  "main": "src/index.js",
  "directories": {
    "test": "test",
    "dist": "dist",
    "demos": "demos",
    "src": "src"
  },
  "scripts": {
    "build": "npm run cleanDist && npm run rollupBuild && npm run babelBuild && npm run minify",
    "cleanDist": "rm -f ./dist/*.*",
    "rollupBuild": "rollup -c rollup.config.js",
    "babelBuild": "babel dist --out-file=dist/fxToolTip.js",
    "test": "tape './test/**/*-test.js'",
    "lint": "eslint .src/*.js",
    "minify": "uglifyjs ./dist/fxToolTip.js -o ./dist/fxToolTip.min.js  -c -m -v",
    "docs": "jsdoc"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-transform-arrow-functions": "^7.0.0",
    "@babel/plugin-transform-object-assign": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "eslint": "^5.5.0",
    "rollup": "^0.66.6",
    "rollup-plugin-node-resolve": "^3.4.0",
    "uglify-js": "^3.4.9"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MichaelRFox/fxToolTip.js.git"
  },
  "keywords": [
    "tooltip",
    "d3.js"
  ],
  "author": "Michael R. Fox",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MichaelRFox/fxToolTip.js/issues"
  },
  "homepage": "https://github.com/MichaelRFox/fxToolTip.js#readme"
}
